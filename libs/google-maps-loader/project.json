{
  "projectType": "library",
  "sourceRoot": "libs/google-maps-loader/src",
  "prefix": "lib",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "tsConfig": "libs/google-maps-loader/tsconfig.lib.json",
        "project": "libs/google-maps-loader/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/google-maps-loader/tsconfig.lib.prod.json"
        }
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "libs/google-maps-loader/src/test.ts",
        "tsConfig": "libs/google-maps-loader/tsconfig.spec.json",
        "karmaConfig": "libs/google-maps-loader/karma.conf.js"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/google-maps-loader/**/*.ts",
          "libs/google-maps-loader/**/*.html"
        ]
      }
    }
  }
}

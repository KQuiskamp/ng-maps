{
  "npmScope": "ng-maps",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "version": {
      "dependsOn": ["^version", "^build"]
    },
    "publish": {
      "dependsOn": ["^build"]
    }
  },
  "cli": {
    "analytics": false
  }
}
